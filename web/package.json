{
  "name": "playhouse-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "rimraf node_modules/apollo-language-server/node_modules/graphql",
    "dev": "npm run gql && next dev",
    "build": "npm run codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "introspect": "apollo client:download-schema",
    "codegen": "apollo client:codegen --target=typescript --localSchemaFile=schema.json",
    "gql": "npm run introspect && npm run codegen",
    "analyze": "ANALYZE=true npm run build",
    "clean:codegen": "find . -name __generated__ -exec rm -rf {} \\;",
    "clean:build": "rm -rf build"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@react-hook/debounce": "^4.0.0",
    "@react-hook/hotkey": "^3.1.0",
    "@reduxjs/toolkit": "^1.5.0",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.20",
    "next": "^12.0.9",
    "phoenix": "^1.5.7",
    "prismjs": "^1.24.0",
    "react": "^17.0.2",
    "react-alert": "^7.0.2",
    "react-canvas-confetti": "^1.2.0",
    "react-colorful": "^5.1.4",
    "react-dom": "^17.0.2",
    "react-h5-audio-player": "^3.7.1",
    "react-hook-form": "^7.4.2",
    "react-modal": "^3.12.1",
    "react-modal-sheet": "^1.4.0",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.2",
    "react-tooltip": "^4.2.13",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "uifx": "^2.0.7"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.1.1",
    "@types/gtag.js": "^0.0.8",
    "@types/lodash": "^4.14.168",
    "@types/node": "^17.0.10",
    "@types/phoenix": "^1.5.1",
    "@types/prismjs": "^1.16.5",
    "@types/react": "^17.0.37",
    "@types/react-alert": "^5.2.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-modal": "^3.10.6",
    "@types/react-redux": "^7.1.16",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.7",
    "apollo": "^2.33.9",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.7",
    "graphql": "^15.7.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
